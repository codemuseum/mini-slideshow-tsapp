<div class="imageElement">
	<h3><%=h slide.title %></h3>
	<p></p>
	<a<%= slide.link.blank? ? " href=\"#\" title=\"\"" : " href=\"#{h(slide.link)}\" title=\"More...\"" %> class="open"></a>
	<%= image_tag slide.picture_url, :class => 'full' %>
	<%= image_tag slide.picture_url, :class => 'thumbnail' %>
</div>